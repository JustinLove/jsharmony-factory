<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<title><%=global.site_title%> - <%= title %></title>
<link href='https://fonts.googleapis.com/css?family=Droid+Sans' rel='stylesheet' type='text/css' />
<link href='https://fonts.googleapis.com/css?family=PT+Sans' rel='stylesheet' type='text/css' />
<link rel="stylesheet" type="text/css" href="/system.css" />
<script type="text/javascript"><%-req.getBaseJS()%></script>
<script type="text/javascript" src="/js/main.js"></script>
<% if(req.isAuthenticated){ %><script type="text/javascript" src="/js/browser.update.js"></script><% } %>
<script type="text/javascript" src="/js/site.js"></script>
<script type="text/javascript" src="/system.js"></script>
<script type="text/javascript" src="/jquery-ui/js/jquery-ui-1.10.3.custom-aspa.min.js"></script>
<link rel="stylesheet" href="/jquery-ui/css/jquery-ui-1.10.3.custom.min.css" />
<script type="text/javascript" src="/js/jquery.colorbox-min.js"></script>
<link rel="stylesheet" href="/js/colorbox/colorbox.css" />
</head>

<body>
<div id="xmain">

<div id="xhead">
  <% if(req.isAuthenticated){ %>
    <div id="xtoplinks"><img src="/images/toplinks_corner.png" id="xtoplinks_corner" alt="" title="" />
      <b><%=global.site_title%>:</b>&nbsp; <%=req.user_name%> &nbsp;|&nbsp; 
      <a id="toplink_help" href="<%-ejsext.getHelpURL(req, jsh, modelid)%>" onclick="<%-ejsext.getHelpOnClick(req,jsh)%>">Help</a> &nbsp;|&nbsp; 
      <a href="/logout">Logout</a>
    </div>
  <% } %>
  <a id="xbrand_link" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/">&nbsp;</a>
  <div id="xlogo">
    <a id="xlogoimg" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/"><img src="/images/logo.png" alt="<%=global.site_title%>" title="<%=global.site_title%>" /></a>
    <a id="xsublogo" href="<%-ejsext.iif(req.isAuthenticated,'',global.home_url)%>/"><img src="/images/portal_admin.png" /></a>
  </div>
  
  <%
  var xmenu_headings = {};
  if('MainMenu' in XMenu){
    %><div id="xmenu"><%
    	for(var i=0;i<XMenu.MainMenu.length;i++){ menu = XMenu.MainMenu[i];
    	  xmenu_headings[menu.ID] = menu.Title;
    	  %><a href="<%-menu.Link%>" onclick="<%-menu.OnClick%>" id="menu_<%=menu.ID%>" class="<%-ejsext.iif(menu.Selected,'selected')%>"><%=menu.Title%><div class="bottom"></div></a><%
      }
    	%><a id="xmenu_more" href="#"><div id="xmenu_more_bg">Menu <img src="/images/arrow_right_white.png" /><div class="bottom"></div></div></a><%
    %></div><%
    %><div id="xmenuside"></div><%
  }
  if('SubMenus' in XMenu){
    for(var parentid in XMenu.SubMenus){ var submenu = XMenu.SubMenus[parentid];
      var parentname = '';
      if(parentid in xmenu_headings) parentname = xmenu_headings[parentid];
      %><div id="xsubmenu_<%=parentid%>" style="display:none;" class="xsubmenu"><div class="head"><%=parentname%>:</div><%
      for(var i=0;i<submenu.length;i++){ menu = submenu[i];
        %><a href="<%-menu.Link%>" onclick="<%-menu.OnClick%>"><%=menu.Title%></a><%-ejsext.iif(i<submenu.length-1,'<div>&nbsp;|&nbsp;</div>')%><%
      }
      %><a class="xsubmenu_more" href="#">Menu &nbsp;&#9656;</a><%
      %></div><%
      %><div id="xsubmenuside"></div><%
    }
  }
  %>
</div>

<div id="xbody"><%-ejsext.BreadCrumbs(req, jsh, modelid)%><% if(title){ %>
<h1 style="display:inline-block;" id="xform_title"><%= title %></h1>
<% } %><%- body %>
</div><!-- xbody -->

<div class="clear"></div>
</div><!--xmain-->
</body>
</html>