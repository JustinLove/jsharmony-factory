{
 "POPUP_EXEC_CREATE_TABLE": {
  "inherits": "POPUP_EXEC",
  "title": "Table Create",
  "onload":"$('#get_codeschema').text(XExt.UndefinedBlank(_GET['codeschema']));$('#get_codename').text(XExt.UndefinedBlank(_GET['codename']));$('#get_codemean').text(XExt.UndefinedBlank(_GET['codemean']));",
  "fields": [
    {"name":"question",
     "value":"Are you sure you want to create table <span id='get_codeschema'></span>.ucod_<span id='get_codename'></span> ? <br /> <span id='get_codemean'></span>"},
    {"name":"codeschema","type":"varchar","length":"16","actions":"BU",
     "default": "js:XExt.UndefinedBlank(_GET['codeschema'])","control":"hidden"},
    {"name":"codename","type":"varchar","length":"16","actions":"BU",
     "default": "js:XExt.UndefinedBlank(_GET['codename'])","control":"hidden"},
    {"name":"codemean","type":"varchar","length":"128","actions":"BU",
     "default": "js:XExt.UndefinedBlank(_GET['codemean'])","control":"hidden"}
  ]
 }, 
 "POPUP_EXEC_CREATE_UCOD": {
  "inherits": "POPUP_EXEC_CREATE_TABLE",
